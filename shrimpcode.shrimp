fun readF(name)
{
	const file = open(name);
	println(file);
	const res = file.read();
	file.close();
	return res;
}

fun writeF(name, cont)
{
	const file = open(name, "w");
	println(file);
	file.write(cont);
	file.close();
}

const fName = "test.shrimp";

const cont = readF(fName);
println(cont);
